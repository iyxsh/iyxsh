# 生产环境配置
# 这些配置将覆盖.env中的同名配置

# 生产环境API基础URL - 使用相对路径，由Web服务器代理到后端API
VITE_APP_API_BASE_URL=/api

# 应用环境标识
VITE_APP_ENV=production

# API请求超时时间（毫秒）
VITE_APP_API_TIMEOUT=30000

# 是否启用调试模式（生产环境应设为false）
VITE_APP_DEBUG=false

# 是否启用性能监控
VITE_APP_PERFORMANCE_MONITORING=true

# 是否启用错误追踪
VITE_APP_ERROR_TRACKING=true